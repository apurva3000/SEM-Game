class Player {
  
  String name;
  int score;
  
  
  Player(String name, int score){
    
    this.name = name;
    this.score = score;
    
    
  }
  
  
  void setScore(int score){
    
    this.score = score;
    
  }
  
  String getName(){
    
   
   return this.name;
   
  }
  
  
  int getScore(){
    
   
   return this.score; 
    
  }
     
 
  
  
}
